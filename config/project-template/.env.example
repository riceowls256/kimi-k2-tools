# Project-specific Kimi K2 Configuration
# Copy this file to .env and update with your values

# Required: Your Moonshot AI API key
ANTHROPIC_AUTH_TOKEN=sk-your-moonshot-api-key-here

# Optional: API endpoint (default: https://api.moonshot.ai/anthropic/)
ANTHROPIC_BASE_URL=https://api.moonshot.ai/anthropic/

# Optional: Enable debug logging (true/false)  
KIMI_DEBUG=false
ANTHROPIC_LOG=

# Optional: Project-specific settings
PROJECT_NAME=my-project
DAILY_SPENDING_LIMIT=2.00
MONTHLY_SPENDING_LIMIT=50.00